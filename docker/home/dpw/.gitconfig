[user]
    name = darryl west
    email = darryl.west@RainCitySoftware.com
[color]
    diff = auto
    status = auto
    branch = auto
    interactive = auto
[apply]
    whitespace = nowarn
[alias]
    st = status
    com = commit
    co = checkout
    br = branch
    hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
[branch]
    autosetupmerge = true
[core]
    excludesfile = /Users/dpw/.gitignore_global
[push]
    default = matching
[pull]
	rebase = false
[init]
	defaultBranch = main
